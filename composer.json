{
    "name": "ritaco/links",
    "description": "Link Manager addon for Rita 3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "plugin", "link", "link-manager", "rita", "ritaco"],
    "homepage": "https://github.com/RitaCo/rita-links-addon.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Saleh Souzanchi",
            "role": "Author",
            "homepage": "http://www.ritaco.net"
        }
    ],    
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Rita/Links\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rita/Links\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support":{
        "source": "https://github.com/RitaCo/rita-links-addon",
        "issues": "https://github.com/RitaCo/rita-links-addon/issues"
    },
    "extra": {
        "installer-name": "Rita/Links"
    }    
}
